<!--pages/subPack/canvas2d/index.wxml-->
<view class="head_box flex-row">
  <block wx:for="{{data_list}}" wx:key="data_list">
    <view class="h_item">
      <canvas2d-ring type="2d" id="can{{index}}" canvasWidth="{{80}}" f_weight="bold"  value="{{item.value}}"  valueColor="#333" f_size="15"  lineWidth="{{3}}"  title="{{index==0?'susu':''}}"></canvas2d-ring>
    </view>
  </block>
</view>
<view class="new_ca">
  <canvas type="2d"  id="myCanvas"  style="width:{{canvasW}}px;height: {{canvasH}}px;"  ></canvas>
</view>
